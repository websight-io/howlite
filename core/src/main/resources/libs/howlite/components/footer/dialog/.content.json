{
  "jcr:primaryType": "nt:unstructured",
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "jcr:primaryType": "nt:unstructured",
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "imageTab": {
      "jcr:primaryType": "nt:unstructured",
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Logo",
      "lgImageSrc": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "wcm/dialogs/components/assetreference",
        "mimeTypes": ["image/*"],
        "name": "image/lgImageSrc",
        "label": "Choose image"
      },
      "alt": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "image/alt",
        "label": "Alt text"
      },
      "showLink": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "image/showLink",
        "label": "Link"
      },
      "container": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "wcm/dialogs/components/container",
        "url": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/pathpicker",
          "rootPath": "/content",
          "name": "image/url",
          "label": "URL"
        },
        "openInNewTab": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/toggle",
          "name": "image/openInNewTab",
          "label": "Open link in a new tab"
        },
        "ws:display": {
          "jcr:primaryType": "nt:unstructured",
          "condition": {
            "jcr:primaryType": "nt:unstructured",
            "sourceName": "image/showLink",
            "values": true
          }
        }
      }
    },
    "menuTab": {
      "jcr:primaryType": "nt:unstructured",
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Menu",
      "items": {
        "jcr:primaryType": "nt:unstructured",
        "name": "navigation/menuItems",
        "label": [
          "Tabs",
          {
            "jcr:primaryType": "nt:unstructured",
            "sling:resourceType": "wcm/dialogs/components/textfield",
            "name": "label",
            "label": "Label"
          }
        ],
        "sling:resourceType": "wcm/dialogs/components/multifield",
        "link": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/pathpicker",
          "rootPath": "/content",
          "name": "link",
          "label": "URL"
        },
        "newtab": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/checkbox",
          "name": "openInNewTab",
          "label": "Open link in a new tab"
        }
      }
    },
    "socialLinksTab": {
      "jcr:primaryType": "nt:unstructured",
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Social links",
      "items": {
        "jcr:primaryType": "nt:unstructured",
        "name": "socialLinks/socialLinkItems",
        "sling:resourceType": "wcm/dialogs/components/multifield",
        "title": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "title",
          "label": "Title"
        },
        "url": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "url",
          "label": "URL"
        },
        "icon": {
          "jcr:primaryType": "nt:unstructured",
          "sling:resourceType": "wcm/dialogs/components/select",
          "name": "icon",
          "label": "Icon",
          "linkedin": {
            "jcr:primaryType": "nt:unstructured",
            "sling:resourceType": "wcm/dialogs/components/select/selectitem",
            "label": "LinkedIn",
            "value": "icon-linkedin"
          },
          "facebook": {
            "jcr:primaryType": "nt:unstructured",
            "sling:resourceType": "wcm/dialogs/components/select/selectitem",
            "label": "Facebook",
            "value": "icon-facebook"
          },
          "twitter": {
            "jcr:primaryType": "nt:unstructured",
            "sling:resourceType": "wcm/dialogs/components/select/selectitem",
            "label": "Twitter",
            "value": "icon-twitter"
          },
          "instagram": {
            "jcr:primaryType": "nt:unstructured",
            "sling:resourceType": "wcm/dialogs/components/select/selectitem",
            "label": "Instagram",
            "value": "icon-instagram"
          }
        }
      }
    },
    "copyrightTab": {
      "jcr:primaryType": "nt:unstructured",
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Copyrights",
      "copyright": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "wcm/dialogs/components/richtext",
        "name": "copyright",
        "label": "Copyrights",
        "configuration": "howlite/components/common/rte/configuration/simple"
      }
    }
  }
}