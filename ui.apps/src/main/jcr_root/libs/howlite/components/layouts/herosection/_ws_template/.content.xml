<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:ws="http://ds.pl/websight"
          jcr:primaryType="nt:unstructured"
          classes="hl-page-section--featured-banner">
      <container 
          jcr:primaryType="nt:unstructured"
          smColSize="12"
          mdColSize="12"
          lgColSize="6"
          sling:resourceType="howlite/components/container">
          <title jcr:primaryType="nt:unstructured"
              title="Add your main title here"
              headingLevel="h1"
              subtitle="Add your subtitle here"
              showSubtitle="true"
              sling:resourceType="howlite/components/title" />
          <ctaslist 
            jcr:primaryType="nt:unstructured"
            sling:resourceType="howlite/components/ctaslist">
                <cta1 jcr:primaryType="nt:unstructured"
                  link="https://test.com"
                  label="Link"
                  classes="hl-cta--primary"
                  sling:resourceType="howlite/components/cta" />
                <cta2 jcr:primaryType="nt:unstructured"
                  link="https://test.com"
                  label="Link"
                  classes="hl-cta--text"
                  sling:resourceType="howlite/components/cta" />
          </ctaslist>
      </container>
        <image
            jcr:primaryType="nt:unstructured"
            smColSize="12"
            mdColSize="12"
            lgColSize="6"
            sling:resourceType="howlite/components/image"/>
</jcr:root>
