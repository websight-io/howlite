<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:ws="http://ds.pl/websight" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="ws:Page">
    <jcr:content
        jcr:primaryType="ws:PageContent"
        jcr:title="Logo cloud"
        sling:resourceType="howlite/components/page"
        ws:template="/libs/howlite/templates/contentpage">
        <rootcontainer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="howlite/components/rootcontainer/fixedrootcontainer">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="howlite/components/headerreference"/>
            <maincontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="howlite/components/maincontainer">
                <pagesection
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <logoscloud
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/logoscloud">
                        <image1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                        <image2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                        <image3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                    </logoscloud>
                </pagesection>
                <pagesection_1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <logoscloud
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/logoscloud">
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                        <image1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            alt="test"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            openInNewTab="true"
                            showLink="true"
                            url="https://www.test.com"/>
                        <image2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            openInNewTab="false"
                            showLink="true"
                            url="https://www.test.com"/>
                        <image3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                        <image_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                        <image_2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/landscape.jpg"
                            showLink="false"/>
                    </logoscloud>
                </pagesection_1>
                <pagesection_2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <logoscloud
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/logoscloud"
                        lgColSize="4"
                        lgOffset="4"
                        mdColSize="10"
                        mdOffset="1"
                        smColSize="10"
                        smOffset="1">
                        <image1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/portrait.jpg"
                            showLink="false"/>
                        <image2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/portrait.jpg"
                            showLink="false"/>
                        <image3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/portrait.jpg"
                            showLink="false"/>
                        <image1_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/portrait.jpg"
                            showLink="false"/>
                        <image2_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/image"
                            lgImageSrc="/content/howlite-test/assets/portrait.jpg"
                            showLink="false"/>
                    </logoscloud>
                </pagesection_2>
                <pagesection_3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <logoscloud
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/logoscloud"
                        lgColSize="10"
                        lgOffset="1"
                        mdColSize="10"
                        mdOffset="1"
                        smColSize="10"
                        smOffset="1"/>
                </pagesection_3>
            </maincontainer>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="howlite/components/footerreference"/>
        </rootcontainer>
    </jcr:content>
</jcr:root>
