<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:ws="http://ds.pl/websight" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="ws:Page">
    <jcr:content
        jcr:primaryType="ws:PageContent"
        jcr:title="Cards"
        sling:resourceType="howlite/components/page"
        ws:template="/libs/howlite/templates/contentpage">
        <rootcontainer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="howlite/components/rootcontainer/fixedrootcontainer">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="howlite/components/headerreference"/>
            <maincontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="howlite/components/maincontainer">
                <pagesection
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <cardslist
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/cardslist">
                        <card1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"
                            ctaClasses="hl-cta--text"/>
                        <card2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"
                            ctaClasses="hl-cta--text"/>
                        <card3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"
                            ctaClasses="hl-cta--text"/>
                    </cardslist>
                </pagesection>
                <pagesection_1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <cardslist
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/cardslist"
                        classes="hl-cards-list--text-centered"
                        headingLevel="h3"
                        headingSize="hl-title__heading--size-3"
                        isSlider="true"
                        itemsPerRowMd="2"
                        itemsPerRowSm="1"
                        itemsPerRowLg="3">
                        <card1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaClasses="hl-cta--primary"
                            ctaDisplayIcon="false"
                            ctaLabel="Link"
                            ctaOpenInNewTab="false"/>
                        <card2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaClasses="hl-cta--secondary"
                            ctaDisplayIcon="false"
                            ctaLabel="Link"
                            ctaOpenInNewTab="false"/>
                        <card3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            title="Third card item"
                            ctaClasses="hl-cta--small"
                            ctaDisplayIcon="false"
                            ctaLabel="Link"
                            ctaOpenInNewTab="false"/>
                        <card
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            title="Last card item"/>
                    </cardslist>
                </pagesection_1>
                <pagesection_2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <cardslist
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/cardslist"
                        headingLevel="h2"
                        headingSize="hl-title__heading--size-4"
                        isSlider="false"
                        lgColSize="6"
                        mdColSize="10"
                        smColsize="10"
                        lgOffset="3"
                        mdOffset="1"
                        smOffset="1">
                        <card1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"/>
                        <card2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"/>
                        <card3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"/>
                    </cardslist>
                </pagesection_2>
                <pagesection_3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <cardslist
                        jcr:primaryType="nt:unstructured"
                        lgColSize="8"
                        mdColSize="10"
                        smColsize="10"
                        lgOffset="2"
                        mdOffset="1"
                        smOffset="1"
                        sling:resourceType="howlite/components/cardslist"/>
                </pagesection_3>
                <pagesection_4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="howlite/components/pagesection">
                    <cardslist
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="howlite/components/cardslist"
                        headingLevel="h2"
                        headingSize="hl-title__heading--size-4"
                        isSlider="false"
                        itemsPerRowLg="2">
                        <card1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"/>
                        <card2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"/>
                        <card3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"
                            ctaLabel="Link"/>
                        <card_2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_5
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_4
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_8
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_7
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                        <card_6
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="howlite/components/cardslist/card"/>
                    </cardslist>
                </pagesection_4>
            </maincontainer>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="howlite/components/footerreference"/>
        </rootcontainer>
    </jcr:content>
</jcr:root>
